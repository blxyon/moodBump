<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unientrepproj.entrep"
    >



    <uses-permission android:name="android.permission.INTERNET" />
    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Entrep">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Entrep.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TabsClasses.QuestionsDiaryActivity"></activity>
        <activity android:name="com.unientrepproj.entrep.StartingPage"></activity>
        <activity android:name="com.unientrepproj.entrep.ResultClasses.ResultActivity"></activity>
        <activity android:name=".CalendarActivity"></activity>
    </application>

</manifest>