<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unientrepproj.entrep">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Entrep">
        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.unientrepproj.entrep.StartingPage"></activity>
        <activity android:name="com.unientrepproj.entrep.Diary"></activity>
        <activity android:name="com.unientrepproj.entrep.Questions"></activity>
        <activity android:name="com.unientrepproj.entrep.Results"></activity>
    </application>

</manifest>